<?xml version="1.0" encoding="utf-8"?>

<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:padding="@dimen/_10sdpq"
    android:background="#F3F9FF">


    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:padding="@dimen/_10sdpq"
        android:orientation="horizontal">


        <ImageView
            android:id="@+id/back"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/_10sdpq"
            android:src="@drawable/ic_left_arrow" />


        <TextView
            android:id="@+id/title"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerInParent="true"
            android:fontFamily="@font/poppins_bold"
            android:text="Photography offers"
            android:textColor="@color/bold_black"
            android:textFontWeight="600"
            android:textSize="@dimen/_20sdpq" />


    </RelativeLayout>



    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content">



     <EditText
        android:id="@+id/search_edit"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginStart="@dimen/_10sdpq"
        android:layout_marginTop="@dimen/_15sdpq"
        android:layout_marginEnd="@dimen/_10sdpq"
        android:background="@drawable/phvotography_edit_bg"
        android:drawableRight="@drawable/search_png"
        android:textFontWeight="400"
        android:textColorHint="@color/text_regular"
        android:drawablePadding="@dimen/_15sdpq"
        android:fontFamily="@font/product_snas_regular"
        android:hint="Search Vendors"
        android:padding="@dimen/_13sdpq" />

    <LinearLayout
        android:id="@+id/category_linear"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@id/search_edit"
        android:layout_marginStart="@dimen/_10sdpq"
        android:layout_marginTop="@dimen/_10sdpq"
        android:layout_marginEnd="@dimen/_10sdpq"
        android:orientation="horizontal">

        <HorizontalScrollView
            android:visibility="gone"
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerHorizontal="true"
                    android:background="@drawable/category_photography_vactor"
                    android:fontFamily="@font/poppins_bold"
                    android:gravity="center"
                    android:padding="@dimen/_5sdpq"
                    android:text="All"
                    android:textColor="#D83973"
                    android:textSize="@dimen/_12sdpq" />

                <TextView
                    android:layout_width="42dp"
                    android:layout_height="18dp"
                    android:layout_gravity="center"
                    android:layout_marginStart="@dimen/_10sdpq"
                    android:gravity="center"
                    android:text="1K-10K"
                    android:textColor="#A7B4C0"
                    android:textSize="@dimen/_9sdpq" />

                <TextView
                    android:layout_width="42dp"
                    android:layout_height="18dp"
                    android:layout_gravity="center"
                    android:layout_marginStart="@dimen/_10sdpq"
                    android:gravity="center"
                    android:text="1K-10K"
                    android:textColor="#A7B4C0"
                    android:textSize="@dimen/_9sdpq" />

                <TextView
                    android:layout_width="42dp"
                    android:layout_height="18dp"
                    android:layout_gravity="center"
                    android:layout_marginStart="@dimen/_10sdpq"
                    android:gravity="center"
                    android:text="1K-10K"
                    android:textColor="#A7B4C0"
                    android:textSize="@dimen/_9sdpq" />

                <TextView
                    android:layout_width="42dp"
                    android:layout_height="18dp"
                    android:layout_gravity="center"
                    android:layout_marginStart="@dimen/_10sdpq"
                    android:gravity="center"
                    android:text="1K-10K"
                    android:textColor="#A7B4C0"
                    android:textSize="@dimen/_9sdpq" />

                <TextView
                    android:layout_width="42dp"
                    android:layout_height="18dp"
                    android:layout_gravity="center"
                    android:layout_marginStart="@dimen/_10sdpq"
                    android:gravity="center"
                    android:text="1K-10K"
                    android:textColor="#A7B4C0"
                    android:textSize="@dimen/_9sdpq" />

                <TextView
                    android:id="@+id/k_15k"
                    android:layout_width="42dp"
                    android:layout_height="18dp"
                    android:layout_gravity="center"
                    android:layout_marginStart="@dimen/_10sdpq"
                    android:gravity="center"
                    android:text="1K-10K"
                    android:textColor="#A7B4C0"
                    android:textSize="@dimen/_9sdpq" />
            </LinearLayout>
        </HorizontalScrollView>

    </LinearLayout>

